
<div class="container px-5 px-sm-0">
    <div class="row">
        <div class="col-md-3">
           <%= render "books/list",book:@book,user:@user %>
        </div>
        <div class="col-md-8 offset-md-1">
            <h2>Users</h2>
            <table class="table">
                 <tr>
                   <th>image</th>
                   <th>name</th>
                   <th></th>
                 </tr>
                <% @user_all.each do |user| %>
                <tr>
                    <td>
                        <%= image_tag user.get_profile_image(100,100) %>
                    </td>
                    <td>
                       <%= user.name %>
                    </td>
                    <td><%= link_to "Show",user_path(user.id) %></td>
                </tr>
                <% end %>
            </table>
        </div>
    </div>
</div>





